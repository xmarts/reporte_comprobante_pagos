<odoo>
  <data>
      <record id="id_complemento" model="ir.ui.view">
      <field name="name">account.payment.complemento</field>
      <field name="model">account.payment</field>
      <field name="inherit_id" ref="account.view_account_payment_form"/>
      <field name="arch" type="xml">
        <xpath expr="//group" position="after">
          <notebook invisible="1"> 
            <page string="Complemento" >

              <field name="complemento_ids" >
                <tree editable="bottom">
                 <field name="nodo"/>
                 <field name="id_documento"/>
                 <field name="serie"/>
                 <field name="folio"/>
                 <field name="modena"/>
                 <field name="parcialidad"/>
                 <field name="metodo"/>
                 <field name="s_anterior"/>
                 <field name="s_pagado"/>
                 <field name="s_insoluto"/>              
                </tree>
              
              </field>
                
            </page> 
          </notebook> 
        </xpath> 
           <xpath expr="//button[@name='action_draft']" position="after"> 
            <button string='cargar complemento' name="complemento" type="object" class="oe_highlight oe_read_only" />
        </xpath> 
      </field>
    </record>
  </data>
</odoo>